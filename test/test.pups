local incl std;
local incl const math util;

decl x y;
make int y = 7;

if ({std:is_null} x) {
    local make str msg = {x is NULL};
    print msg;
};

if ({std:any_null} x y) {
    local make str msg = {There is at least one NULL in x, y};
    print msg;
} else {
    local make str msg = {There isn't any NULL in x, y};
    print msg;
};
