{
  "name": "pourupscript",
  "displayName": "PourUPScript",
  "description": "Simple PUPS functionalities",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pups",
      "aliases": ["PourUPScript", "pups"],
      "extensions": [".pups"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pups",
      "scopeName": "source.pups",
      "path": "./syntaxes/pups.tmLanguage.json"
    }]
  }
}
