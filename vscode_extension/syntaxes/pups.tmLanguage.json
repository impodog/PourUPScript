{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "PourUPScript",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#types"
		},
		{
			"include": "#variables"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.pups",
					"match": "\\b(decl|print|set|local|scope|nothing|incl|comment|null|make|again|add_path|new|rmv|ret|if|else|true|false|while|do|number|using)\\b"
				}
			]
		},
		"types": {
			"patterns": [
				{
					"name": "entity.name.type.class",
					"match": "\\b(int|flo|str|byte|schar|fn|anyt|typet|nullt|lambda|std)\\b"
				}
			]
		}
	},
	"scopeName": "source.pups"
}